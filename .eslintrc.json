{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-typescript-prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "consistent-return": "off"
    }
}
